{
    "alias": {
        "build": {
            "commands": [
                [
                    "exec",
                    "--in-project",
                    "python3",
                    "setup.py",
                    "bdist_wheel"
                ]
            ],
            "documentation": null
        },
        "clean": {
            "commands": [
                [
                    "exec",
                    "--in-project",
                    "rm",
                    "-rf",
                    "dist",
                    "build"
                ]
            ],
            "documentation": null
        },
        "deploy": {
            "commands": [
                [
                    "upload"
                ]
            ],
            "documentation": "Another name for uploading the package"
        },
        "flow": {
            "commands": [
                [
                    "upload"
                ]
            ],
            "documentation": null
        },
        "test.bash": {
            "commands": [
                [
                    "test",
                    "exec",
                    "bash"
                ]
            ],
            "documentation": null
        },
        "test.create-docker-image": {
            "commands": [
                [
                    "exec",
                    "--in-project",
                    "docker",
                    "build",
                    "-t",
                    "clk",
                    "."
                ]
            ],
            "documentation": null
        },
        "test.exec": {
            "commands": [
                [
                    "exec",
                    "docker",
                    "exec",
                    "-ti",
                    "clk-test"
                ]
            ],
            "documentation": null
        },
        "test.flow": {
            "commands": [
                [
                    "test",
                    "run"
                ]
            ],
            "documentation": null
        },
        "upload": {
            "commands": [
                [
                    "exec",
                    "--in-project",
                    "twine",
                    "upload",
                    "dist/*"
                ]
            ],
            "documentation": null
        }
    },
    "flowdeps": {
        "build": [
            "clean"
        ],
        "upload": [
            "build"
        ]
    },
    "parameters": {
        "flow": [
            "--flow"
        ],
        "test.flow": [
            "--flow"
        ]
    }
}
