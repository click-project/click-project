{
    "alias": {
        "test-n-push": {
            "commands": [
                [
                    "exec",
                    "--in-project",
                    "earthly",
                    "+local-sanity-check"
                ],
                [
                    "exec",
                    "--in-project",
                    "git",
                    "push"
                ]
            ],
            "documentation": null
        }
    },
    "flowdeps": {
        "build": [
            "clean"
        ],
        "upload": [
            "build"
        ]
    },
    "parameters": {
        "flow": [
            "--flow"
        ],
        "test.flow": [
            "--flow"
        ]
    }
}
